<%- include('./includes/head.ejs') %>

<body id="net">
    <div>
        <img src="../img/calculator.png" class="rounded mx-auto d-block" alt="calculator" width="500px" height="500px">
    </div>
    <h2 class="text-center">All In One Networking Calculator</h2>

    <div class="row" style="margin-left: 27%;">
            <div class="card shadow-lg p-3 mb-5 bg-white rounded" style="width: fit-content;">
               
                <div class="button-icon-btn">
                    <a class="btn text-white" style="background-color: #3b5998;" href="/ip" role="button">
                        <i class="fas fa-calculator"></i>
                    IPv4 Calculator  </a>
                    <a class="btn btn-secondary btn-info" href="/calculatorView"><i class="fas fa-calculator"></i> Scientific Calculator</a>
                    <a class="btn btn-secondary btn-warning" href="/binary"><i class="fas fa-calculator"></i> Binary Calculator</a> 
                    <a class="btn text-white" style="background-color: #3b9851;" href="/mathPage"><i class="fas fa-calculator"></i> Math Calculator</a> 
                </div>     

                <div class="button-icon-btn">
                    <a class="btn btn-secondary btn-info" href="/web"><i class="fas fa-calculator"></i> Website Bandwidth Calculator</a>
                    <a class="btn btn-secondary btn-danger" href="/"><i class="fas fa-calculator"></i> Data Unit Converter</a>
                    <a class="btn btn-secondary btn-dark" href="/generator"><i class="fas fa-calculator"></i> Password Generator</a>   
            </div>
        </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r121/three.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vanta@latest/dist/vanta.net.min.js"></script>
    <script>
    VANTA.NET({
      el: "#net",
      mouseControls: true,
      touchControls: true,
      gyroControls: false,
      height: 600.00,
      minWidth: 200.00,
      scale: 1.00,
      scaleMobile: 1.00,
      color: 0x0,
      backgroundColor: 0xeeeef2
    })
    </script>
